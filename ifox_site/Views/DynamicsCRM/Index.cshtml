<script>
    // Ensure that Swiper is initialized once the DOM is fully loaded
    document.addEventListener('DOMContentLoaded', function () {
        const swiper = new Swiper('.swiper-container', {
            loop: true,
            speed: 600,
            autoplay: {
                delay: 5000, // Adjust the delay for autoplay (in milliseconds)
                disableOnInteraction: false, // Ensure autoplay continues after user interaction
            },
            slidesPerView: 'auto',
            spaceBetween: 40,
            pagination: {
                el: '.swiper-pagination',
                clickable: true,
                type: 'bullets',
            },
            navigation: {
                // Disable navigation buttons by not setting any buttons in the configuration
                nextEl: null,
                prevEl: null
            },
            breakpoints: {
                320: {
                    slidesPerView: 2,
                    spaceBetween: 40,
                },
                480: {
                    slidesPerView: 3,
                    spaceBetween: 60,
                },
                640: {
                    slidesPerView: 4,
                    spaceBetween: 80,
                },
                992: {
                    slidesPerView: 5,
                    spaceBetween: 120,
                },
                1200: {
                    slidesPerView: 6,
                    spaceBetween: 120,
                },
            },
        });
    });
</script>

